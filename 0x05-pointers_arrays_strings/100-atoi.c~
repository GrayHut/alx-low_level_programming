#include "main.h"


int _atoi(char *s)
{
	int x, i = 0;
	int m = 1;
	unsigned int n = 0

		while (s[x])
		{
			if (s[x] == 45)
			{
				m *= -1;
			}

			while (s[x] >= 48 && s[x] <= 57)
			{
				i = 1;
				n = (n * 10) + (s[x] - '0');
				c++;
			}
			if (i == 1)
			{
				break;
			}

			x++
		}
	n *= m;
	return (n);
}
